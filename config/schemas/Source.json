{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
      "District": {
        "$ref": "#/definitions/EntityTypeConfig"
      },
      "State": {
        "$ref": "#/definitions/EntityTypeConfig"
      },
      "Block": {
        "$ref": "#/definitions/EntityTypeConfig"
      },
      "SubDistrict": {
        "$ref": "#/definitions/EntityTypeConfig"
      },
      "Village": {
        "$ref": "#/definitions/EntityTypeConfig"
      },
      "City": {
        "$ref": "#/definitions/EntityTypeConfig"
      }
    },
    "definitions": {
      "EntityTypeConfig": {
        "type": "object",
        "patternProperties": {
          "^[a-zA-Z0-9]+$": {
            "$ref": "#/definitions/SourceConfig"
          }
        }
      },
      "SourceConfig": {
        "type": "object",
        "properties": {
          "keyMap": {
            "$ref": "#/definitions/KeyMap"
          }
        },
        "required": ["keyMap"]
      },
      "KeyMap": {
        "type": "object",
        "properties": {
          "districtCode": { "type": "string" },
          "name": { "type": "string" },
          "higherHierarchy": { "type": "string" }
        },
        "required": ["districtCode", "name", "higherHierarchy"]
      }
    },
    "required": ["District", "State", "Block", "SubDistrict", "Village", "City"]
  }
  