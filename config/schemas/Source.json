{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
      "source": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/definitions/SourceConfig"
        }
      }
    },
    "required": ["source"],
    "title": "Source Config",
    "definitions": {
      "SourceConfig": {
        "type": "object",
        "properties": {
          "entity": {
            "type": "string"
          },
          "keyMap": {
            "$ref": "#/definitions/KeyMap"
          }
        },
        "required": ["entity", "keyMap"],
        "additionalProperties": false
      },
      "KeyMap": {
        "type": "object",
        "properties": {
          "districtCode": { "type": "string" },
          "name": { "type": "string" },
          "higherHierarchy": { "type": "string" }
        },
        "required": ["districtCode", "name", "higherHierarchy"]
      }
    }
  }
  